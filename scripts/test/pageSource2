<script type="text/javascript">
function byId(id) {
  return document.getElementById(id);
}

function vote(node) {
  var v = node.id.split(/_/);   // {'up', '123'}
  var item = v[1]; 

  // hide arrows
  byId('up_'   + item).style.visibility = 'hidden';
  byId('down_' + item).style.visibility = 'hidden';

  // ping server
  var ping = new Image();
  ping.src = node.href;

  return false; // cancel browser nav
} </script><title>Hacker News</title></head><body><center><table border=0 cellpadding=0 cellspacing=0 width="85%" bgcolor=#f6f6ef><tr><td bgcolor=#ff6600><table border=0 cellpadding=0 cellspacing=0 width="100%" style="padding:2px"><tr><td style="width:18px;padding-right:4px"><a href="http://ycombinator.com"><img src="y18.gif" width=18 height=18 style="border:1px #ffffff solid;"></img></a></td><td style="line-height:12pt; height:10px;"><span class="pagetop"><b><a href="news">Hacker News</a></b><img src="s.gif" height=1 width=10><a href="newest">new</a> | <a href="newcomments">comments</a> | <a href="ask">ask</a> | <a href="jobs">jobs</a> | <a href="submit">submit</a></span></td><td style="text-align:right;padding-right:4px;"><span class="pagetop"><a href="newslogin?whence=%2f%78%3f%66%6e%69%64%3d%4d%49%6d%6e%72%64%74%6e%31%32%33%59%4f%41%6e%52%68%61%57%69%7a%74">login</a></span></td></tr></table></td></tr><tr><td colspan=2><table border=0 cellpadding=0 cellspacing=0 width="100%" style="padding:2px;"><tr><td><span class="pagetop"><div align="center"><a href="https://thedaywefightback.org">The Day We Fight Back</a></div></span></td></tr></table></td></tr><tr style="height:10px"></tr><tr><td><table border=0 cellpadding=0 cellspacing=0><tr><td align=right valign=top class="title">331.</td><td><center><a id=up_7211286 href="vote?for=7211286&amp;dir=up&amp;whence=%2f%78%3f%66%6e%69%64%3d%4d%49%6d%6e%72%64%74%6e%31%32%33%59%4f%41%6e%52%68%61%57%69%7a%74"><div class="votearrow" title="upvote"></div></a><span id=down_7211286></span></center></td><td class="title"><a href="http://cryto.net/~joepie91/blog/2014/02/10/why-mtgox-is-full-of-shit/">Why Mt. Gox is full of shit</a><span class="comhead"> (cryto.net) </span></td></tr><tr><td colspan=2></td><td class="subtext"><span id=score_7211286>266 points</span> by <a href="user?id=klrr">klrr</a> 1 day ago  | <a href="item?id=7211286">132 comments</a></td></tr><tr style="height:5px"></tr><tr><td align=right valign=top class="title">